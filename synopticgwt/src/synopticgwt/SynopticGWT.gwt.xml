<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='synopticgwt'>
  <!-- Inherit the core Web Toolkit stuff. -->
  <inherits name='com.google.gwt.user.User'/>
  
  <!-- See http://code.google.com/webtoolkit/doc/latest/DevGuideLogging.html -->
  <!--  <inherits name="com.google.gwt.logging.Logging"/> -->

  <!--  Uncomment the next line to disable logging -->
  <!--  <set-property name="gwt.logging.enabled" value="FALSE"/> -->
  <!--  <set-property name="gwt.logging.logLevel" value="FINEST"/> -->

  <!--  <set-property name="gwt.logging.logLevel" value="FINEST"/> -->
  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <inherits name='com.google.gwt.user.theme.standard.Standard'/>
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

  <!-- Other module inherits                                      -->

  <!-- Specify the app entry point class.                         -->
  <entry-point class='synopticgwt.client.SynopticGWT'/>

  <!--  Compile ONLY for firefox -->
  <!--  <set-property name="user.agent" value="gecko1_8" /> -->


  <!-- <extend-property name="user.agent" values="safari" /> -->

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>
  <source path='tests'/>
  
  <!-- Specify the path to any remote services.                   -->
  <servlet path="/synoptic" class="synopticgwt.server.SynopticService" />

</module>
